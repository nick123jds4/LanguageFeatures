<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <link href="~/lib/bootstap/dist/css/bootstrap.min.css" rel="stylesheet" />
    <title>@ViewBag.Title</title>
</head>
<body class="m-1 p-1">
    <div>
        <nav class="navbar navbar-expand-lg bg-body-tertiary">
            <div class="container-fluid">
                <a class="navbar-brand" href="#">Панель навигации</a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Переключатель навигации">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="collapse navbar-collapse" id="navbarNav">
                    <ul class="navbar-nav">
                        <li class="nav-item">
                            <a class="nav-link active" aria-current="page" asp-action="Index">Главная</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" asp-controller="Home" asp-action="List">Список</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" asp-action="Details">Детали</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" asp-action="DoList">Список с префиксом</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link disabled">Отключенная</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>

    </div>
    <div>
        @RenderBody()
    </div>
</body>
</html>
